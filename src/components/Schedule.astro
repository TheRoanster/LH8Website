---
const eventObject = [
  { startTime: "7:00AM", endTime: "7:45AM", Core: "Set up"},
  { startTime: "7:45AM", endTime: "8:00AM", Core: "Check-in"},
  { startTime: "8:00AM", endTime: "9:00AM", Core: "Opening Ceremony", Event: "Declaring Category" },
  { startTime: "9:00AM", endTime: "10:00AM", Core: "Hacking Begins", Event: "First Timer Workshop" },
  { startTime: "10:00AM", endTime: "11:00AM", Core: "", Event: "Group Formations" },
  { startTime: "11:00AM", endTime: "12:00PM", Core: "", Event: "Tech Panel" },
  { startTime: "12:00PM", endTime: "1:00PM", Core: "", Event: "Lunch" },
  { startTime: "1:00PM", endTime: "2:00PM", Core: "", Event: "Escape Room" },
  { startTime: "2:00PM", endTime: "3:00PM", Core: "", Event: "" },
  { startTime: "3:00PM", endTime: "4:00PM", Core: "", Event: "Dodgeball" },
  { startTime: "4:00PM", endTime: "5:00PM", Core: "", Event: "" },
  { startTime: "5:00PM", endTime: "6:00PM", Core: "", Event: "Dinner" },
  { startTime: "6:00PM", endTime: "7:00PM", Core: "", Event: "" },
  { startTime: "7:00PM", endTime: "7:30PM", Core: "Code Freeze/Create Your Presentations", Event: "Jeopardy" },
  { startTime: "7:30PM", endTime: "8:00PM", Core: "Presentations", Event: "Karoake" },
  { startTime: "8:00PM", endTime: "9:00PM", Core: "Presentations/Judging Discussions", Event: "" },
  { startTime: "9:00PM", endTime: "9:30PM", Core: "Award Ceremony", Event: "" },
];
---

<div class="w-full mx-auto px-6 md:px-12 xl:px-6 bg-black ">
  <div class="relative pb-10 pt-10 sm:pt-10 sm:pb-10 ml-auto min-h-full flex flex-col items-center">
    <h1 id="schedule" class="font-header text-white font-semibold text-3xl md:text-4xl xl:text-5xl mt-12 mb-4">Schedule</h1>
    <div class="w-lg max-w-full overflow-x-auto">
      <table class="w-full text-sm text-left text-gray-500 dark:text-gray-400 overflow-x-auto">
        <thead class="text-base uppercase bg-black text-gray-100">
          <tr>
            <th scope="col" class="px-6 py-3">Time</th>
            <th scope="col" class="px-6 py-3">Core</th>
            <th scope="col" class="px-6 py-3">Event</th>
          </tr>
        </thead>
        <tbody class="text-white">
          {
            eventObject.map((element) => (
              <tr class="bg-black">
                <td class="px-6 py-4 font-medium text-lg whitespace-nowrap">
                  {element.startTime} - {element.endTime}
                </td>
                <td class="px-6 py-4 font-medium text-lg whitespace-nowrap">{element.Core}</td>
                <td class="px-6 py-4 font-medium text-lg whitespace-nowrap">{element.Event}</td>
              </tr>
            ))
          }
        </tbody>
      </table>
    </div>
  </div>
</div>
